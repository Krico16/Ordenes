                                        <form action="/ordenes/new" method="POST">
                                            <div class="form-group">
                                                <label for="company" class=" form-control-label">Nombre</label>
                                                <input type="text" name="nombre" id="nombre" placeholder="Nombre del proyecto" class="form-control">
                                            </div>
